// Adjusted commands to ensure addresses are within valid range
// Covers random, sequential, and edge-case scenarios

// High-frequency READ/WRITE in block 0
WRITE bin 0000000000000001
WRITE bin 0000000000000010
READ bin 0000000000000001
READ bin 0000000000000010
WRITE bin 0000000000000100
READ bin 0000000000000111

// Sequential READ/WRITE in block 10
WRITE 163880
WRITE 163890
READ 163880
READ 163890
WRITE 163900
READ 163910

// Sparse READ/WRITE to random blocks
WRITE 900000
READ 700000
WRITE 800000
READ 600000

// Frequent access to hotspot address in block 15
WRITE 245760
READ 245760
WRITE 245760
READ 245760

// Cross-page boundary operations
WRITE bin 0111111111111111
READ bin 1000000000000000
WRITE bin 1000000000000001
READ bin 1000000000000010
WRITE bin 1000000000000100

// Simulate sequential READ/WRITE at upper edge
WRITE 1048000
READ 1048500
WRITE 1048501
READ 1048575

// Bursty READ/WRITE in block 20
WRITE 327680
WRITE 327681
READ 327680
WRITE 327682
READ 327681
READ 327682

// Simulate mixed operations
READ 300000
WRITE 350000
READ 400000
WRITE 450000

// Frequent re-access of recently modified pages
WRITE bin 101010101010
READ bin 101010101010
WRITE bin 101010101011
READ bin 101010101011
WRITE bin 101010101100
READ bin 101010101100

// Testing random sparse access with valid upper addresses
WRITE 900000
READ 850000
WRITE 950000
READ 999999
